<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="work_list_template" name="周报列表">
        <div id="wrap" class="container">
            <h1>工作周报</h1>
            <t t-foreach="works" t-as="work">
                <div class="row">
                    <span t-field="works.name" />,
                    <span t-field="works.date_start" />,
                    <span t-field="works.publisher_id" />
                </div>
            </t>
        </div>
    </template>
</odoo>